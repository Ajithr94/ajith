def fact(a):
    s=1
    while (a!=1):
     s=s*a
     a=a-1;
    return s
print(fact(4))

def sinterest(P,R,T):
    s=(P*R*T)/100
    return s
print(sinterest(5000,2,2))

def cinterest(p,r,t):
    a=p*(pow((1+r/100),t))
    c=a-p
    return c
print(cinterest(1000,10,2))

sum = 0  
num =154
temp = num
while temp > 0:  
   digit = temp % 10  
   sum += digit ** 3  
   temp //= 10  
  
if num == sum:  
   print(num,"is an Armstrong number")  
else:  
   print(num,"is not an Armstrong number")  

